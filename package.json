{
  "name": "@maikolib/vite-plugin-yaml",
  "version": "1.1.1-0",
  "description": "Import YAML files as JS objects ðŸ”Œ, remove all pnpm stuffs",
  "type": "module",
  "author": {
    "name": "Maiko Tan",
    "email": "maiko.tan.coding@gmail.com",
    "url": "https://github.com/tonyketcham"
  },
  "license": "MIT",
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./modules": {
      "types": "./modules.d.ts"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "types",
    "*.d.ts"
  ],
  "keywords": [
    "vite",
    "vite-plugin",
    "yaml",
    "yml",
    "modyfi"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MaikoTan/vite-plugin-yaml"
  },
  "bugs": {
    "url": "https://github.com/MaikoTan/vite-plugin-yaml/issues"
  },
  "homepage": "https://github.com/MaikoTan/vite-plugin-yaml/tree/main/#readme",
  "peerDependencies": {
    "vite": ">=3.2.7"
  },
  "dependencies": {
    "@rollup/pluginutils": "5.1.0",
    "js-yaml": "4.1.0",
    "tosource": "2.0.0-alpha.3"
  },
  "devDependencies": {
    "@types/js-yaml": "4.0.9",
    "c8": "8.0.1",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "vite": "5.0.13"
  },
  "packageManager": "yarn@4.5.1",
  "publishConfig": {
    "tag": "latest"
  }
